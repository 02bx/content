<def-group>
  <definition class="compliance"
  id="selinux_policy_enforcing" version="1">
    <metadata>
      <title>Enable SELinux</title>
      <affected family="unix">
        <platform>Red Hat Enterprise Linux 6</platform>
      </affected>
      <reference ref_id="CCE-3624-4" source="CCE" />
      <description>The SELinux policy should be set
      appropriately.</description>
    </metadata>
    <criteria>
      <criterion test_ref="test_20107" />
    </criteria>
  </definition>
  <ind:textfilecontent54_test check="all"
  check_existence="all_exist"
  comment="Tests the value of the ^[\s]*SELINUXTYPE[\s]*=[\s]*([^#]*) expression in the /etc/selinux/config file"
  id="test_20107" version="1">
    <ind:object object_ref="obj_20107" />
    <ind:state state_ref="state_20107" />
  </ind:textfilecontent54_test>
  <ind:textfilecontent54_state id="state_20107"
  version="1">
    <ind:subexpression operation="equals" var_check="all"
    var_ref="var_20107" />
  </ind:textfilecontent54_state>
  <external_variable comment="External variable for definition 20107"
  datatype="string" id="var_20107"
  version="1" />
  <ind:textfilecontent54_object id="obj_20107"
  version="1">
    <ind:path>/etc/selinux</ind:path>
    <ind:filename>config</ind:filename>
    <ind:pattern operation="pattern match">^[\s]*SELINUXTYPE[\s]*=[\s]*([^#\s]*)</ind:pattern>
    <ind:instance datatype="int">1</ind:instance>
  </ind:textfilecontent54_object>
</def-group>
