<def-group>
  <definition class="compliance" id="service_rlogin_disabled"
  version="1">
    <metadata>
      <title>Service rlogin Disabled</title>
      <affected family="unix">
        <platform>Red Hat Enterprise Linux 6</platform>
      </affected>
      <reference ref_id="3537-8" source="CCE" />
      <description>The rlogin service should be disabled if
      possible.</description>
    </metadata>
    <criteria operator="AND"
    comment="service rlogin is not configured to start">
      <criterion comment="rlogin runlevel 0"
      test_ref="test_runlevel0_rlogin" />
      <criterion comment="rlogin runlevel 1"
      test_ref="test_runlevel1_rlogin" />
      <criterion comment="rlogin runlevel 2"
      test_ref="test_runlevel2_rlogin" />
      <criterion comment="rlogin runlevel 3"
      test_ref="test_runlevel3_rlogin" />
      <criterion comment="rlogin runlevel 4"
      test_ref="test_runlevel4_rlogin" />
      <criterion comment="rlogin runlevel 5"
      test_ref="test_runlevel5_rlogin" />
      <criterion comment="rlogin runlevel 6"
      test_ref="test_runlevel6_rlogin" />
    </criteria>
  </definition>
  <unix:runlevel_test check="all" check_existence="any_exist"
  comment="Runlevel test" id="test_runlevel0_rlogin"
  version="2">
    <unix:object object_ref="obj_runlevel0_rlogin" />
    <unix:state state_ref="state_service_off" />
  </unix:runlevel_test>
  <unix:runlevel_test check="all" check_existence="any_exist"
